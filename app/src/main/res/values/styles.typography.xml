        <item name="android:textSize">@dimen/font_size_1</item>
    </style>

    <style name="TMedium" parent="TextAppearance.AppCompat.Medium">
        <item name="fontFamily">@font/tt_commons_regular</item>
        <item name="android:textSize">@dimen/font_size_2</item>
